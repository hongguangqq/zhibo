<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.jyt.baseapp.view.activity.LivePlayActivity">
    <com.netease.nim.chatroom.demo.thirdparty.video.NEVideoView
        android:id="@+id/video_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="centerCrop"/>
    <com.netease.nimlib.sdk.avchat.model.AVChatSurfaceViewRenderer
        android:id="@+id/video_render"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</RelativeLayout>
